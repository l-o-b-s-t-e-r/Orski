<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.scene.chart.BarChart?>
<?import javafx.scene.chart.CategoryAxis?>
<?import javafx.scene.chart.NumberAxis?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.BorderPane?>
<BorderPane xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.company.Controller">
   <top>
      <Button mnemonicParsing="false" onMouseClicked="#loadDataSet" text="Load" BorderPane.alignment="CENTER" />
   </top>
   <center>
       <BarChart fx:id="chart" prefHeight="500" prefWidth="1000" BorderPane.alignment="CENTER" animated="false">
         <xAxis>
             <CategoryAxis side="BOTTOM" />
         </xAxis>
         <yAxis>
             <NumberAxis side="LEFT" />
         </yAxis>
       </BarChart>
   </center>
</BorderPane>
